{
  "name": "vscode-newsheadline",
  "displayName": "vscode-NewsHeadline",
  "description": "",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-newsheadline.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "title": "vscode-NewsHeadline",
      "properties": {
        "vscode-NewsHeadline.newsSource": {
          "type": "array",
          "default": ["https://www.nhk.or.jp/rss/news/cat0.xml"],
          "description": "News Sources"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/jsdom": "^20.0.0",
    "@types/node-fetch": "^2.6.2",
    "axios": "^0.27.2",
    "jsdom": "^20.0.0"
  },
  "repository": {
    "type": "git"
  }
}
